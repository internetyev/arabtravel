{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": true,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true
	},
	"exclude": [
		"admin/static",
		"core",
		"node_modules",
		"sql",
		"static",
		"systemd",
		"tpl",
		"typings/main",
		"typings/main.d.ts"
	]
}
